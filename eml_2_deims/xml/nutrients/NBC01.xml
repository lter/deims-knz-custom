  <dataset>
    <alternateIdentifier>NBC01</alternateIdentifier>
    <title>NBC01 Konza Prairie Belowground Plot Experiment: Soil Chemistry responses to experimental manipulations of fire, nutrients and mowing</title>
    <creator>
      <individualName>
        <givenName>John</givenName>
