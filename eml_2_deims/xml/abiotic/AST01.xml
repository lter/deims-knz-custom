	<dataset>
		<alternateIdentifier>AST01</alternateIdentifier>
		<title>AST01 Soil Temperature Measured in Burned, Burned-clipped, and Unburned Plots at Konza Prairie</title>
		<creator>
			<individualName>
				<givenName>John</givenName>
				<givenName>M.</givenName>
				<surName>Briggs</surName>
			</individualName>
			<address>
